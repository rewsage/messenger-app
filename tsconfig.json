{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"target": "ES6",
		"jsx": "react",
		"strict": true,
		"allowJs": true,
		"noImplicitReturns": true,
        "moduleResolution": "node",
        "baseUrl": "./src",
        "paths": {
            "@/*": ["./*"]
        }
	},
    "include": ["./src/**/*"]
}

